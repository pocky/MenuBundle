{% extends "BlackMenuBundle:AdminMenu:base.html.twig" %}
{% block content_top %}
    {% include 'BlackAdminBundle:Admin:contentTop.html.twig' with {'title': "menu.admin.contentTop.new.text"} %}
{% endblock %}
{% block page_content %}
<div class="fluid">

    <form id="validate" class="main grid9" action="{{ path(admin_new) }}"  method="post" {{ form_enctype(form) }}>
        <fieldset>
            {% include 'BlackAdminBundle:AdminForm:fieldset.html.twig' with {'form': form, 'title': "menu.admin.new.text"} %}
        </fieldset>
    </form>
    {% include 'BlackAdminBundle:AdminForm:actions.html.twig' %}
</div>
{% include 'BlackMenuBundle:AdminMenu:actions.html.twig' with {'document': document} %}
{% endblock %}